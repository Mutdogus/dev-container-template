      "containerImage": "mcr.microsoft.com/vscode/devcontainers/javascript-node:18",
      "extensions": ["ms-vscode.cpptools"],
      "expectedExtensions": 1,
      "timeout": 30000
    },
    {
      "name": "multi-extension-test",
      "vscodeVersion": "stable",
      "containerImage": "mcr.microsoft.com/vscode/devcontainers/javascript-node:18",
      "extensions": ["ms-python.python", "ms-vscode.vscode-typescript-next"],
      "expectedExtensions": 2,
      "timeout": 45000
    },
    {
      "name": "insiders-test",
      "vscodeVersion": "insiders",
      "containerImage": "mcr.microsoft.com/vscode/devcontainers/javascript-node:18",
      "extensions": ["ms-vscode.cpptools"],
      "expectedExtensions": 1,
      "timeout": 30000
    }
  ],
